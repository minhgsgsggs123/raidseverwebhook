# MÃ u sáº¯c embed
colors = [0xE74C3C, 0x2ECC71, 0xF39C12, 0x9B59B6, 0x3498DB, 0x1ABC9C]
def send_embed():
    # Random chá»n username, avatar, title, mÃ u
    username = random.choice(usernames)
    avatar = random.choice(avatars)
    title = random.choice(titles)
    color = random.choice(colors)
    
    # Gá»­i Táº¤T Cáº¢ tin nháº¯n cÃ¹ng lÃºc (khÃ´ng random)
    description = "\n".join(messages)
    
    # Ná»™i dung hiá»ƒn thá»‹ bÃªn ngoÃ i embed
    content = "chÃ o máº¥y con gÃ  cÃ´ng ngiá»‡p ngu lá»“n Ã³c chÃ³ğŸ˜‚ğŸ˜‚ğŸ˜‚ sever bá»‘ mÃ y nek https://discord.gg/euEzQfCc2j \n@everyone <@678344927997853742>"
    # Payload gá»­i webhook
    payload = {
        "username": username,
        "avatar_url": avatar,
        "content": content,
        "embeds": [
            {
                "title": title,
                "description": description,
                "color": color
            }
        ]
    }
    
    # Gá»­i Ä‘áº¿n Táº¤T Cáº¢ webhook cÃ¹ng lÃºc
    for i, webhook_url in enumerate(webhook_urls):
        try:
            response = requests.post(webhook_url, json=payload)
            print(f"[{response.status_code}] Sent to webhook {i+1} from {username}")
        except Exception as e:
            print(f"Lá»—i webhook {i+1}: {e}")
# Gá»­i 30 láº§n (má»—i láº§n gá»­i Ä‘áº¿n Táº¤T Cáº¢ webhook)
for _ in range(30):
    send_embed()
    time.sleep(0.5)  # Delay 0.5 giÃ¢y giá»¯a cÃ¡c láº§n gá»­i
